#include<stdio.h>
int main()
{
  int n, sum=0;

  printf("Enter n value: \n");
  scanf("%d", &n);

  for(int i=0; i<=n; i++)
  {
  	 printf("The squares of %d numbers is: %d\n",n,i*i);
     sum += (i*i);
  }

  printf("Sum of squares of first %d natural numbers = %d\n",n, sum);

  return 0;
}